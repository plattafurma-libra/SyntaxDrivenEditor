 328 PROCEDURE charGroup (VAR range:Range);
 329 BEGIN (*charGroup*)
 330         IF (*sym=neg*) iden="^" THEN range.pos:=FALSE; GetSym(); 
 331         ELSE range.pos:=TRUE END;
 332         posCharGroup(range);
 333 END charGroup;
