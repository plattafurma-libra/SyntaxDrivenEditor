 201 PROCEDURE SingleCharEsc (VAR char:CHAR); 
 202 BEGIN
 203         CASE sym OF
 204           escape: char:="\";
 205         | bar: char:="|";
 206         | strich: char:="-";
 207         | osquareBr: char:="[";
 208         | csquareBr: char:="]";
 209         | neg: char:="^";
 210         | lKlammer: char:="(";
 211         | rKlammer: char:=")";
 212         | glKlammer: char:="{";
 213         | grKlammer: char:="}";
 214         | punkt: char:=".";
 215         | plus: char:="+";
 216         | asterisk: char:="*";
 217         | qum: char:="?";
 218         ELSE 
 219                 IF iden="n" THEN char:=0AX; 
 220                 ELSIF iden="r" THEN char:=0DX;
 221                 ELSIF iden="t" THEN char:=09X;
 222                 ELSE (* Interface_Halt.halt.HaltPar(127); *) Error(2);
 223                  END;
 224         END;
 225         GetSym();
 226 END SingleCharEsc;
