 113 PROCEDURE Read();
 114 BEGIN
 115         ch:=regString^[i];INC(i);
 116 END Read;
 117 
 118 PROCEDURE GetSym();
 119 BEGIN
 120         CASE ch OF              
 121         CHR(1)..CHR(39),
 122         CHR(44),
 123         CHR(47)..CHR(62),
 124         CHR(64)..CHR(90), 
 125         CHR(94  )..CHR(122),
 126         CHR(126)..CHR(255):sym:=char;iden:=ch; Read(); (*alles außer Metachar*)
 127         | "*":sym:=asterisk; Read();
 128         | "+":sym:=plus; Read();
 129         | "?":sym:=qum; Read();
 130         | "|":sym:=bar; Read();
 131         | "[":sym:=osquareBr; Read();
 132         | "]":sym:=csquareBr; Read();
 133         | "\":sym:=escape; Read();
 134         | "(":sym:=lKlammer; Read();
 135         | ")":sym:=rKlammer; Read();
 136         | "{":sym:=glKlammer; Read();
 137         | "}":sym:=grKlammer; Read();
 138         | "-":sym:=strich;Read();  (*auch nicht Metachar; Algorithmus Ã¤ndern *)
 139         | ".":sym:=punkt;Read();
 140         ELSE sym:=other
 141         END
 142 END GetSym;
